<!DOCTYPE html>
<html lang="en">
<head>
   <%- include ('partials/head.ejs') %>
</head>
<body>
    <div class="container">
        <div class="head">CONTRIBUTIONS</div>
        <div class="part-con" id="particles-js">
            <div class="sub-container">
                <% repos.forEach(function(i) { %> 
                    <div class="card">
                        <div class="project-name"><%= i.fullName %></div>
                        <p class="desc" title="<%= i.url %> "><%= i.description %></p>
                        <div class="seperator"></div>
                        <div class="lang"><%= i.language %></div>
                    </div>    
                <% }); %> 
            </div>
            <%- include('partials/nav.ejs')  %>
        </div> 
    </div>
    <%- include('partials/scripts.ejs') %> 
</body>
</html>