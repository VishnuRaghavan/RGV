<!-- load particles when navigated to corresponding pages-->
<% if (title === 'RGV') { %> 
<script src="/js/particles.js"></script>
<script>
    // loading particles 
    particlesJS.load('particles-js','/js/lines_bubbles.json', ()=>{
        console.log('particles json loaded successfuly');
    });    
</script>
<% } else if(title === 'WORK' || title === 'CONTRIBUTIONS') { %>
<script src="/js/particles.js"></script>
<script>
    // loading particles 
    particlesJS.load('particles-js','/js/space.json', ()=>{
        console.log('particles json loaded successfuly');
    });    

    $(document).ready(() => {
        const subContainerHeight = $('.sub-container').outerHeight(true);
        const navHeight = $('.nav').outerHeight(true);
        $('#particles-js').innerHeight(subContainerHeight + navHeight);
    });

</script>
<% } %> 
<!-- load particles when navigated to corresponding pages -->

